{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to NestJS in Docker",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "address": "localhost",
            "restart": true,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": []
        }
    ]
}